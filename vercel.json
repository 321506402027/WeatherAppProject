{
    "builds": [{
        "src": "WeatherProject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "WeatherProject/wsgi.py"
        }
    ]
}
{
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.10"
    }
  }
}

